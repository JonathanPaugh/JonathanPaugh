{
  "profiles": {
    "StaticServer": {
      "commandName": "Project",
      "commandLineArgs": "--http 8000"
    },
    "LiveServer": {
      "commandName": "Executable",
      "executablePath": "dotnet.exe",
      "workingDirectory": "$(ProjectDir)",
      "commandLineArgs": "watch run --http 8000",
      "applicationUrl": "http://localhost:8000",
      "launchBrowser": true,
    }
  }
}